CXX = g++
COMPILE_FLAGS = -O3

sample_program_2/main: sample_program_2/main.o sample_program_2/greet.o sample_program_2/greet.h
	$(CXX) -o sample_program_2/main sample_program_2/main.o sample_program_2/greet.o

sample_program_2/main.o: sample_program_2/main.cpp sample_program_2/greet.h
	$(CXX) -c -o sample_program_2/main.o sample_program_2/main.cpp $(COMPILE_FLAGS)

sample_program_2/greet.o: sample_program_2/greet.cpp sample_program_2/greet.h
	$(CXX) -c -o sample_program_2/greet.o sample_program_2/greet.cpp $(COMPILE_FLAGS)

